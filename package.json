{
  "name": "misskey-em",
  "version": "1.0.0-alpha.2",
  "description": "A CLI app of the text based script language for Misskey Pages",
  "scripts": {
    "build": "npm run peg && npm run tsc",
    "build-debug": "npm run peg-debug && npm run tsc",
    "peg": "mkdirp ./built/parser/peg && pegjs -o built/parser/peg/EmsInternalParser.js src/parser/peg/EmsInternalParser.pegjs",
    "peg-debug": "mkdirp ./built/parser/peg && pegjs -o built/parser/peg/EmsInternalParser.js --trace src/parser/peg/EmsInternalParser.pegjs",
    "tsc": "tsc"
  },
  "bin": {
    "em": "./built/cli/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marihachi/EmeraldScript.git"
  },
  "author": "Marihachi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marihachi/EmeraldScript/issues"
  },
  "dependencies": {
    "@types/semver": "^6.2.0",
    "corekey": "^1.0.0-beta.6",
    "semver": "^7.1.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^12.0.4",
    "@types/uuid": "^3.4.4",
    "mkdirp": "^0.5.1",
    "pegjs": "^0.10.0",
    "typescript": "^3.4.5"
  },
  "files": [
    "built"
  ]
}
