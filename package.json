  "bugs": {
    "url": "https://github.com/marihachi/EmeraldScript/issues"
  },
  "dependencies": {
    "@types/node": "^12.0.3",
    "@types/uuid": "^3.4.4",
    "corekey": "^1.0.0-beta.6",
    "mkdirp": "^0.5.1",
    "node-fetch": "^2.6.0",
    "pegjs": "^0.10.0",
    "typescript": "^3.4.5",
    "uuid": "^3.3.2"
  }
}
