{
  "name": "misskey-em",
  "version": "1.0.0-alpha.4",
  "description": "A CLI app of the text based script language for Misskey Pages",
  "scripts": {
    "build": "npm run peg && npm run tsc",
    "build-debug": "npm run peg-debug && npm run tsc",
    "peg": "mkdirp ./built/language/Emerald && pegjs -o built/language/Emerald/parser.js src/language/Emerald/parser.pegjs",
    "peg-debug": "mkdirp ./built/language/Emerald && pegjs -o built/language/Emerald/parser.js --trace src/language/Emerald/parser.pegjs",
    "tsc": "tsc"
  },
  "bin": {
    "em": "./built/cli/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marihachi/EmeraldScript.git"
  },
  "author": "Marihachi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marihachi/EmeraldScript/issues"
  },
  "dependencies": {
    "@syuilo/aiscript": "^0.11.1",
    "cafy": "15.2.1",
    "corekey": "1.0.x",
    "semver": "7.3.x",
    "uuid": "8.3.x"
  },
  "devDependencies": {
    "@types/node": "14.14.x",
    "@types/pegjs": "0.10.x",
    "@types/semver": "7.3.x",
    "@types/uuid": "8.3.x",
    "mkdirp": "1.0.x",
    "pegjs": "0.10.x",
    "typescript": "4.0.x"
  },
  "files": [
		"built",
		"examples"
  ]
}
